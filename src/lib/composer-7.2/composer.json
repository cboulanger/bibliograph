{
  "config": {
    "platform": {
      "php": "7.2.24"
    }
  },
  "description": "PHP Backend for Bibliograph",
  "minimum-stability": "stable",
  "name": "cboulanger/bibliograph",
  "replace": {
    "bower-asset/inputmask": "*",
    "bower-asset/jquery": "*",
    "bower-asset/punycode": "*",
    "bower-asset/yii2-pjax": "*"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.org",
      "canonical": false
    },
    {
      "type": "vcs",
      "url": "https://github.com/cboulanger/yii2-jsonrpc.git",
      "reference": "bibliograph"
    },
    {
      "type": "vcs",
      "url": "https://github.com/cboulanger/worldcat-linkeddata-php.git"
    }
  ],
  "require": {
    "adci/full-name-parser": "^0.2.3",
    "aracoool/yii2-uuid": "^1.1",
    "bmsrox/yii2-module-autoloader": "^1.1.1",
    "danielstjules/stringy": "^3.1",
    "edvlerblog/yii2-adldap-module": "^6.0.0",
    "ext-gettext": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "ext-zip": "*",
    "hedii/zotero-api": "^1.1",
    "magp/yii2-json-schema-validator": "dev-master",
    "neitanod/forceutf8": "^2.0",
    "renanbr/crossref-client": "^1",
    "rsinger/worldcat-linkeddata-php": "dev-master",
    "symfony/dotenv": "^5.0",
    "toriphes/yii2-console-runner": "^1.2.1",
    "voku/simple-cache": "^3.1",
    "yiisoft/yii2": "^2.0.32",
    "yiisoft/yii2-swiftmailer": "^2.1",
    "yosymfony/toml": "^1.0.0",
    "ext-mysqli": "*",
    "georgique/yii2-jsonrpc": "dev-bibliograph"
  },
  "require-dev": {
    "bizley/migration": "^2.2",
    "codeception/codeception": "^4.0",
    "codeception/module-asserts": "^1.1",
    "codeception/module-phpbrowser": "^1.0.0",
    "codeception/module-rest": "^1.2.0",
    "codeception/module-yii2": "^1.1",
    "codeception/specify": "*",
    "codeception/verify": "*",
    "elisdn/yii2-gii-fixture-generator": "^1.1",
    "goaop/parser-reflection": "^1.4",
    "graze/guzzle-jsonrpc": "^3.0",
    "insolita/yii2-migration-generator": "^2.3",
    "phpunit/phpunit": "<7",
    "yiisoft/yii2-faker": "^2.0.0",
    "yiisoft/yii2-gii": "^2.0.0",
    "odannyc/yii2-sse": "^0.1.0",
    "react/http-client": "^0.5.9",
    "react/stream": "^0.7.7"
  }
}
